<div class="modal fade" id="celular" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="titlecelulares"></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form method="post">
                    <!--Nombre-->
                    <div class="form-group">
                        <label for="name">Nombre</label>
                        <input type="text" class="form-control" id="nameCel" placeholder="Linea">
                    </div>

                    <!--ID-->
                    <div class="form-group">
                        <label for="id">ID</label>
                        <input type="text" class="form-control" id="idNumCel" placeholder="ID">
                    </div>
                    <!--Numero-->
                    <div class="form-group">
                        <label for="numero">Numero</label>
                        <input type="text" class="form-control" id="numCel" placeholder="Numero">
                    </div>
                    <!--Estado-->
                    <div class="form-group">
                        <label for="estado">Estado</label>
                        <select id="estadoCel" class="form-control">
                            <option value="1">Activo</option>
                            <option value="0">Inactivo</option>
                        </select>
                    </div>
                    
                </form>
            </div>
            <div class="modal-footer" style="padding-right: 41%;">
                <button type="submit" class="btn" value="actualizar"id="btnCelular" style="background-color: #00a884; color:white;"></button>
            </div>
        </div>
    </div>
</div>